    "require-dev": {
        "codeception/codeception": "^2.1",
        "phpro/grumphp": "^0.11.6",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "squizlabs/php_codesniffer": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.3"
